  "scripts": {
    "dev": "cross-env NODE_ENV=development next",
    "build": "next build",
    "start": "next start",
    "test": "jest --setupFiles dotenv/config",
    "cypress": "npm run cypress:headless",
    "cypress:open": "cypress open",
    "cypress:headless": "nyc --reporter=text-summary cypress run --headless",
    "cypress:browser": "nyc --reporter=text-summary cypress run --browser chrome --no-exit",
