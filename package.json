    "sonar": "rimraf .scannerwork && node node_modules/sonarqube-scanner/dist/bin/sonar-scanner",
    "ts-coverage": "typescript-coverage-report --threshold=91",
    "prettier": "prettier-eslint --eslint-config-path \"./.eslintrc\" --write \"src/**/*.{js,ts,tsx}\""
